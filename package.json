    "build:android:debug:dev": "ionic cordova build android",
    "build:android": "ionic cordova platform add android && ionic cordova build android",
    "run:android": "npm run build:android && native-run android --app platforms/android/app/build/outputs/apk/debug/app-debug.apk --device",
